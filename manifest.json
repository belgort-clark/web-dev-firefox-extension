{
    "manifest_version": 2,
    "name": "Clark College Web Development",
    "version": "1.0.8",
    "description": "Useful links and resources for Clark College Web Development Students",
    "icons": {
      "32": "icon_32.png",
      "48": "icon_48.png",
      "64": "icon_64.png",
      "128": "icon_128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"]
      }
    ],

    "browser_action": {
      "default_popup": "popup.html"
    }
}